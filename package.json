{
  "name": "AccountableMobileChallenge",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start:prod": "ENV=production react-native start --reset-cache",
    "start:dev": "ENV=development react-native start --reset-cache",
    "build:ios": "react-native run-ios",
    "build:ios:release": "react-native run-ios --configuration Release",
    "build:android": "react-native run-android",
    "build:android:release": "react-native run-android --variant=release",
    "pods": "cd ios && pod install && cd ../",
    "clean:all": "rm -rf node_modules ios/Pods ios/build && npm i && npm run pods",
    "clean:pods": "rm -rf ios/Pods ios/build && npm i && npm run pods",
    "clean:gradle": "cd android && ./gradlew clean && cd .. && npm i",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint --quiet .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/hooks": "^3.0.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/elements": "^1.3.31",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.4.1",
    "@shopify/flash-list": "^1.7.1",
    "@shopify/restyle": "^2.4.4",
    "axios": "^1.7.7",
    "babel-plugin-module-resolver": "^5.0.2",
    "metro-react-native-babel-preset": "^0.77.0",
    "mobx": "^6.13.5",
    "mobx-react": "^9.1.1",
    "mobx-state-tree": "^6.0.1",
    "react": "18.3.1",
    "react-native": "0.76.0",
    "react-native-dotenv": "^3.4.11",
    "react-native-flash-message": "^0.4.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-mmkv": "^3.1.0",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-screens": "^3.35.0",
    "react-native-svg": "^15.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.0-alpha.2",
    "@react-native-community/cli-platform-android": "15.0.0-alpha.2",
    "@react-native-community/cli-platform-ios": "15.0.0-alpha.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/babel-preset": "^0.76.0",
    "@react-native/eslint-config": "0.76.0",
    "@react-native/metro-config": "0.76.0",
    "@react-native/typescript-config": "0.76.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/parser": "^8.11.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "prettier-plugin-organize-imports": "^4.1.0",
    "react-test-renderer": "18.3.1",
    "reactotron-react-native": "^5.1.9",
    "typescript": "5.0.4"
